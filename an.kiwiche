function openChest() {
    const codeInput = document.getElementById('codeInput').value;
    const message = document.getRlementById('message');

   if (codeInput === 'QCKHAKMF') {
    message.innerHTML = 'Chúc mừng! bạn đã mở rương thành công!';
  } else { 
    message.innerHTML = 'Yes sir, mã không chuẩn rồi. Vui lòng thử lại nha.';
  }
}
